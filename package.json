{
  "name": "wc-synchrotron",
  "version": "1.0.0",
  "description": "The new JavaScript and API powered interface for WooCommerce",
  "author": "WooThemes",
  "license": "SEE LICENSE IN LICENSE file",
  "keywords": [
    "woo",
    "woocommerce",
    "wordpress",
    "wordpress",
    "react",
    "javascript",
    "php"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Automattic/wc-synchrotron.git"
  },
  "bugs": {
    "url": "https://github.com/Automattic/wc-synchrotron/issues"
  },
  "scripts": {
    "test": "npm run build-dev && npm run test-client",
    "test-client": "NODE_ENV=test NODE_PATH=test:client TEST_ROOT=client node test/runner.babel.js",
    "build-dev": "npm run clean && npm run calypso-regenerate-client-dev && npm run build-client-dev",
    "build-client-dev": "node_modules/webpack/bin/webpack.js -d --progress --colors",
    "calypso-regenerate-client-dev": "node_modules/wp-calypso/server/config/regenerate-client.js > node_modules/wp-calypso/client/config/index.js",
    "clean": "rimraf dist",
    "lint": "node node_modules/eslint/bin/eslint.js --cache --ext=.js --ext=.jsx ."
  },
  "homepage": "https://github.com/Automattic/wc-synchrotron#readme",
  "dependencies": {
    "babel-runtime": "^5.8.35",
    "classnames": "1.1.1",
    "commander": "^2.3.0",
    "component-classes": "^1.2.1",
    "debug": "^2.2.0",
    "lodash.omit": "3.1.0",
    "moment": "2.10.6",
    "moment-timezone": "^0.4.0",
    "react": "0.14.3",
    "react-dom": "0.14.3",
    "react-redux": "4.0.1",
    "react-tap-event-plugin": "^0.2.1",
    "react-tooltip": "^1.1.4",
    "redux": "3.0.4",
    "redux-actions": "^0.9.1",
    "redux-effects": "^0.4.3",
    "redux-effects-fetch": "^0.4.5",
    "redux-logger": "^2.6.1",
    "redux-multi": "^0.1.12"
  },
  "devDependencies": {
    "babel": "^5.8.35",
    "babel-core": "^5.8.35",
    "babel-eslint": "^4.1.8",
    "babel-loader": "^5.4.0",
    "chai": "^2.0.0",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.11.3",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "glob": "^7.0.3",
    "mocha": "^2.3.4",
    "node-sass": "^3.4.2",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.14",
    "wp-calypso": "github:automattic/wp-calypso"
  }
}
